{
    "name": "onlime/laravel-bexio-api-client",
    "description": "Client library for Bexio API using Laravel HTTP Client",
    "keywords": [
        "onlime",
        "bexio-api-client",
        "bexio-api-php-client",
        "bexio",
        "laravel",
        "HTTP Client"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Iezzi",
            "email": "philip@onlime.ch"
        }
    ],
    "require": {
        "php": "^8.2",
        "onlime/bexio-api-client": "^0.5",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "suggest": {
        "onlime/laravel-http-client-global-logger": "A zero-configuration logger for Laravel HTTP Client"
    },
    "autoload": {
        "psr-4": {
            "LaravelBexio\\": "src/"
        }
    }
}
